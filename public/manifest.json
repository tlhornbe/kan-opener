{
  "manifest_version": 3,
  "name": "Kan-Opener",
  "version": "1.0.2",
  "description": "A privacy-focused Kanban board for the browser's new tab page.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "search"
  ],
  "icons": {
    "128": "icon128.png"
  }
}